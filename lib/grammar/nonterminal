Inner -> MultiElementGroup | MultiElementOneOf .

MultiElementGroup -> unaryTripleExpr ';' unaryTripleExpr MEG.

MEG ->
    | ';' FUnary .


FUnary ->
   | unaryTripleExpr FAUnary .

FAUnary ->
    | ';' FUnary .

MultiElementOneOf -> Mult '@' Mult FMultiElementOneOf .

FMultiElementOneOf -> '@' Mult FMultiElementOneOf
                   |
                   .

Mult -> unaryTripleExpr FMult .

FMult ->	
   | ';' OFFMult .

OFFMult ->
   | Mult .

